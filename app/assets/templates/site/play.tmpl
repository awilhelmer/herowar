<div class="container">
	<div class="head" data-view="views/play/head"></div>
	<div class="panel mapselect">
		<div class="header">Choose Map</div>
		<ul>
			{{#each []}}
				<li data-id="{{id}}"><span>{{name}}</span></li>
			{{/each}}
		</ul>
	</div>
	{{#if results}}
		<div class="panel results">
			<div class="header">Results</div>
			{{#each results}}
				<div class="entry">
					<span class="cdate">{{formattedCdate}}</span>
					<span>{{lives}} lives left ... </span>
					{{#if victory}}
						<span>Victory</span>
					{{else}}
						<span>Defeat</span>
					{{/if}}
					<span class="score">{{score}}</span>
				</div>
			{{/each}}
		</div>
	{{/if}}
</div>

<footer class="footer" data-view="views/footer"></footer>