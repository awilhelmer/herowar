<div class="header">Coop</div>
<div class="body">
	{{#unless id}}
		<ul>
			<li class="create" data-id="102"><span>Create</span></li>
			<li class="join" data-id="102"><span>Join</span></li>
		</ul>
	{{else}}
		<p>Players:</p>
		{{#each playerResults}}
			Player #{{player.id}}
		{{/each}}
		<div class="buttons">
			<a class="btn btn-primary">Start Game</a>
		</div>
	{{/unless}}
</div>